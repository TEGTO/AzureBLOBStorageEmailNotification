{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerOptions": {
    "ContainerName": "files",
    "MaxFileSize": 10485760,
    "FileNameRegex": "\\d{15}-@",
    "AllowedFileFormats": [ ".docx", ".document" ]
  }
}
